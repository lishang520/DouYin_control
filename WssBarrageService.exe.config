<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<!--过滤Websocket数据源进程,可用','进行分隔，程序将会监听以下进程的弹幕信息-->
		<add key="filterProcess" value="直播伴侣,chrome,msedge"/>
		<!--Websocket监听端口-->
		<add key="wsListenPort" value="8888"/>
		<!--在控制台输出弹幕-->
		<add key="printBarrage" value="on"/>
	</appSettings>
</configuration>